{
  "cli": {
    "version": ">= 16.24.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_SERVER_URL": "@EXPO_PUBLIC_SERVER_URL",
        "EXPO_PUBLIC_OPENAI_API_KEY": "@EXPO_PUBLIC_OPENAI_API_KEY",
        "EXPO_PUBLIC_WHISPER_API_KEY": "@EXPO_PUBLIC_WHISPER_API_KEY"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_SERVER_URL": "@EXPO_PUBLIC_SERVER_URL",
        "EXPO_PUBLIC_OPENAI_API_KEY": "@EXPO_PUBLIC_OPENAI_API_KEY",
        "EXPO_PUBLIC_WHISPER_API_KEY": "@EXPO_PUBLIC_WHISPER_API_KEY"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_SERVER_URL": "@EXPO_PUBLIC_SERVER_URL",
        "EXPO_PUBLIC_OPENAI_API_KEY": "@EXPO_PUBLIC_OPENAI_API_KEY",
        "EXPO_PUBLIC_WHISPER_API_KEY": "@EXPO_PUBLIC_WHISPER_API_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
